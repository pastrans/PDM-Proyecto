<?xml version="1.0" encoding="utf-8"?>
<TableLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingTop="30dip"
    android:stretchColumns="1"
    tools:context=".Activities.Coordinacion.GestionarCoordinacion">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="@string/titGestionarCoordinador"
        android:layout_gravity="center_horizontal"
        android:textSize="30sp" />

    <TableRow>

        <TextView android:text="@string/txtTipoCoordinacion" />
        <EditText
            android:id="@+id/editTipoCoordinacion"
            android:inputType="text"
            android:maxLength="30" />
    </TableRow>

    <Button
        android:layout_width="100dp"
        android:layout_height="wrap_content"
        android:onClick="btnNuevoGCoordinador"
        tools:ignore="OnClick"
        android:text="@string/btnNuevoGCoordinador" />


    <Button
        android:onClick="btnEditarGCoordinador"
        android:text="@string/btnEditarGCoordinador"
        tools:ignore="OnClick" />

    <Button
        android:onClick="btnEliminarGCoordinador"
        android:text="@string/btnEliminarGCoordinador"
        tools:ignore="OnClick" />

    <Button
        android:onClick="btnRegresarGCoordinador"
        android:text="@string/btnRegresarGCoordinador"
        tools:ignore="OnClick" />

</TableLayout>